<h3>Student Form</h3>
<%= form_tag student_path do %>
    <label for="student_first_name">"Student's First Name"</label><br>
    <%= text_field_tag :'student[first_name]' %><br>
    <label for="student_last_name">"Student's Last Name"</label><br>
    <%= text_field_tag :'student[last_name]' %><br>
    <%= submit_tag "Submit Student" %>
<%= end %>